<ns:tables xsi:schemaLocation="http://labkey.org/data/xml ..\..\..\..\..\schemas\tableInfo.xsd" xmlns:ns="http://labkey.org/data/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <ns:table tableName="Calibrations" tableDbType="TABLE">
      <ns:description>Contains one row for each calibration parameter from each scan imported into this folder.</ns:description>
      <ns:columns>
        <ns:column columnName="ScanId">
            <ns:description>Contains the id of the scan.</ns:description>
            <ns:isHidden>true</ns:isHidden>
        </ns:column>
        <ns:column columnName="Name">
            <ns:description>Contains the name of the calibration parameter.</ns:description>
        </ns:column>
        <ns:column columnName="Value">
            <ns:description>Contains the value of the calibration parameter.</ns:description>
        </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="Features" tableDbType="TABLE">
      <ns:description>Contains one row per feature identified in each MS1 features file imported into this folder.</ns:description>
      <ns:columns>
        <ns:column columnName="FeatureId">
            <ns:isHidden>true</ns:isHidden>
            <ns:description>Contains the unique id of the feature.</ns:description>
        </ns:column>
        <ns:column columnName="FileId">
            <ns:columnTitle>Features File</ns:columnTitle>
            <ns:isUnselectable>true</ns:isUnselectable>
            <ns:description>Contains the unique id of the file from which this feature was imported (see the Files table).</ns:description>
        </ns:column>
        <ns:column columnName="Scan">
            <ns:description>Contains the scan number during which the feature appeared at its highest intensity.</ns:description>
        </ns:column>
        <ns:column columnName="Time">
          <ns:formatString>#,##0.0000</ns:formatString>
          <ns:description>Contains the time offset (in seconds) at which the feature was detected.</ns:description>
        </ns:column>
        <ns:column columnName="MZ">
            <ns:formatString>#,##0.0000</ns:formatString>
            <ns:description>Contains the mass-to-charge ratio for the identified feature.</ns:description>
        </ns:column>
        <ns:column columnName="AccurateMZ">
          <ns:formatString>Y;N</ns:formatString>
          <ns:description>Contains true when the m/z value came directly from a peak in the (centroided) source spectrum; false otherwise </ns:description>
        </ns:column>
        <ns:column columnName="Mass">
          <ns:formatString>#,##0.0000</ns:formatString>
          <ns:description>Contains the calculated mass of the feature.</ns:description>
        </ns:column>
        <ns:column columnName="Intensity">
            <ns:formatString>#,##0.0000</ns:formatString>
            <ns:description>Contains the intensity of the feature.</ns:description>
        </ns:column>
        <ns:column columnName="Charge">
            <ns:columnTitle>Z</ns:columnTitle>
            <ns:formatString>#+</ns:formatString>
            <ns:description>Contains the estimated charge for the feature.</ns:description>
        </ns:column>
        <ns:column columnName="ChargeStates">
            <ns:columnTitle>Z States</ns:columnTitle>
            <ns:description>Contains the number of charge states in which the feature appeared.</ns:description>
        </ns:column>
        <ns:column columnName="KL">
            <ns:formatString>#,##0.0000</ns:formatString>
            <ns:description>Contains the Kullback-Leibler value, wich measures the similarity between the featureâ€™s peak pattern and the peaks expected from a peptide of that mass.</ns:description>
        </ns:column>
        <ns:column columnName="Background">
            <ns:formatString>#,##0.0000</ns:formatString>
            <ns:description>Contains the The (localized) running minima for intensity calculated across two dimensions (m/z and time (scan)).</ns:description>
        </ns:column>
        <ns:column columnName="Median">
            <ns:formatString>#,##0.0000</ns:formatString>
            <ns:description>Contains the difference between the median and the minima in the resampled data for the feature.</ns:description>
        </ns:column>
        <ns:column columnName="Peaks">
            <ns:description>Contains the number of peaks in the feature.</ns:description>
        </ns:column>
        <ns:column columnName="ScanFirst">
            <ns:description>Contains the first scan number in which the feature appeared.</ns:description>
        </ns:column>
        <ns:column columnName="ScanLast">
            <ns:description>Contains the last scan number in which the feature appeared.</ns:description>
        </ns:column>
        <ns:column columnName="ScanCount">
            <ns:description>Contains the number of scans in which the feature appeared.</ns:description>
        </ns:column>
        <ns:column columnName="TotalIntensity">
            <ns:formatString>#,##0.0000</ns:formatString>
            <ns:description>The summed intensity (across the elution profile) of all deconvoluted peptides in the group.</ns:description>
        </ns:column>
        <ns:column columnName="Description">
            <ns:description>Contains user-supplied comments about the feature.</ns:description>
        </ns:column>
        <ns:column columnName="MS2Scan">
            <ns:description>Contains the scan number of the related MS2 scan found in an associated pep.xml file.</ns:description>
        </ns:column>
        <ns:column columnName="MS2ConnectivityProbability">
            <ns:formatString>0.0000</ns:formatString>
            <ns:columnTitle>MS2 Prob</ns:columnTitle>
            <ns:description>Contains a probability as to how certain the MS2 match is.</ns:description>
        </ns:column>
        <ns:column columnName="MS2Charge">
          <ns:columnTitle>MS2 Z</ns:columnTitle>
          <ns:formatString>#+</ns:formatString>
          <ns:description>Contains the charge of the associated MS2 peptide.</ns:description>
        </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="Files" tableDbType="TABLE">
      <ns:description>Contains a list of MS1 data files that have been imported into this folder.</ns:description>
      <ns:columns>
        <ns:column columnName="FileId">
            <ns:isHidden>true</ns:isHidden>
            <ns:description>Contains the unique id of the data file.</ns:description>
        </ns:column>
        <ns:column columnName="ExpDataFileId">
            <ns:columnTitle>Experiment Data File</ns:columnTitle>
            <ns:isUnselectable>true</ns:isUnselectable>
            <ns:description>Contains the unique id of the associated row in the exp.data table.</ns:description>
        </ns:column>
        <ns:column columnName="Type">
            <ns:description>Contains the type of the file (1=features, 2=peaks).</ns:description>
        </ns:column>
        <ns:column columnName="MzXmlUrl">
            <ns:description>Contains the full path to the source MzXML file from which this data file was produced.</ns:description>
        </ns:column>
        <ns:column columnName="Imported">
            <ns:isHidden>true</ns:isHidden>
            <ns:description>Indicates whether the data flie was fully imported or not.</ns:description>
        </ns:column>
          <ns:column columnName="Deleted">
              <ns:isHidden>true</ns:isHidden>
              <ns:description>Indicates whether the data flie was deleted or not. Deleted files are kept in the database until an MS1 purge occurs.</ns:description>
          </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="PeakFamilies" tableDbType="TABLE">
      <ns:description>Contains one row per peak family identified in each peaks xml file imported into this folder.</ns:description>
      <ns:columns>
        <ns:column columnName="PeakFamilyId">
            <ns:isHidden>true</ns:isHidden>
            <ns:description>Contains the unique peak family id.</ns:description>
        </ns:column>
        <ns:column columnName="ScanId">
            <ns:isHidden>true</ns:isHidden>
            <ns:isUnselectable>true</ns:isUnselectable>
            <ns:description>Contains the associated scan id.</ns:description>
        </ns:column>
        <ns:column columnName="MzMono">
            <ns:formatString>#,##0.0000</ns:formatString>
            <ns:columnTitle>MZ Monoisotopic</ns:columnTitle>
            <ns:description>Contains the monoisotopic mass-to-charge ratio for the peak family.</ns:description>
        </ns:column>
        <ns:column columnName="Charge">
            <ns:columnTitle>Z</ns:columnTitle>
            <ns:description>Contains the overall charge for the peak family.</ns:description>
        </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="Peaks" tableDbType="TABLE">
      <ns:description>Contains a row for each peak listed in every peaks xml file imported into this folder.</ns:description>
      <ns:columns>
        <ns:column columnName="PeakId">
            <ns:isHidden>true</ns:isHidden>
            <ns:description>Contains the unique identifier for the peak.</ns:description>
        </ns:column>
        <ns:column columnName="ScanId">
            <ns:columnTitle>Scan Information</ns:columnTitle>
            <ns:isUnselectable>true</ns:isUnselectable>
            <ns:description>Contains the unique id of the associated scan.</ns:description>
        </ns:column>
        <ns:column columnName="MZ">
            <ns:formatString>#,##0.0000</ns:formatString>
            <ns:description>Contains the mass-to-charge ratio for the peak.</ns:description>
        </ns:column>
        <ns:column columnName="Intensity">
            <ns:formatString>#,##0.0000</ns:formatString>
            <ns:description>Contains the intensity for the peak.</ns:description>
        </ns:column>
        <ns:column columnName="Area">
            <ns:formatString>#,##0.00</ns:formatString>
            <ns:description>Contains the area for the peak.</ns:description>
        </ns:column>
        <ns:column columnName="Error">
            <ns:formatString>#,##0.0000</ns:formatString>
            <ns:description>Contains the estimated error for the peak.</ns:description>
        </ns:column>
        <ns:column columnName="Frequency">
            <ns:formatString>#,##0</ns:formatString>
            <ns:description>Contains the frequence for the peak.</ns:description>
        </ns:column>
        <ns:column columnName="Phase">
            <ns:formatString>#,##0.00</ns:formatString>
            <ns:description>Contains the phase of the peak.</ns:description>
        </ns:column>
        <ns:column columnName="Decay">
            <ns:formatString>#,##0.00</ns:formatString>
            <ns:description>The decay for the peak.</ns:description>
        </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="PeaksToFamilies" tableDbType="TABLE">
      <ns:description>A many-to-many relation table between peaks and peak families.</ns:description>
      <ns:columns>
        <ns:column columnName="PeakFamilyId">
            <ns:columnTitle>Peak Family</ns:columnTitle>
            <ns:isUnselectable>true</ns:isUnselectable>
        </ns:column>
        <ns:column columnName="PeakId">
            <ns:isHidden>true</ns:isHidden>
            <ns:isUnselectable>true</ns:isUnselectable>
        </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="Scans" tableDbType="TABLE">
      <ns:description>Contains all scans found in all peaks xml files imported into this folder.</ns:description>
      <ns:columns>
        <ns:column columnName="ScanId">
            <ns:isHidden>true</ns:isHidden>
            <ns:description>Contains the unique id for the scan.</ns:description>
        </ns:column>
        <ns:column columnName="FileId">
            <ns:columnTitle>Peaks File</ns:columnTitle>
            <ns:isUnselectable>true</ns:isUnselectable>
            <ns:description>Contains the unique id of the associated file.</ns:description>
        </ns:column>
        <ns:column columnName="Scan">
            <ns:description>Contains the scan number for the scan.</ns:description>
        </ns:column>
        <ns:column columnName="RetentionTime">
            <ns:formatString>#,##0.0000</ns:formatString>
            <ns:description>Contains the retention time (in seconds) for the scan.</ns:description>
        </ns:column>
        <ns:column columnName="ObservationDuration">
            <ns:formatString>#,##0.0000</ns:formatString>
            <ns:description>Contains the duration of the scan.</ns:description>
        </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="Software" tableDbType="TABLE">
      <ns:description>Contains information about the software used to produce a given file.
          There will be one row in this table for each software package used to produce the file identified by the FileId column.</ns:description>
      <ns:columns>
        <ns:column columnName="SoftwareId">
            <ns:description>Contains the unique id of the software package.</ns:description>
        </ns:column>
        <ns:column columnName="FileId">
            <ns:description>Contains the unique id of the associated file.</ns:description>
        </ns:column>
        <ns:column columnName="Name">
            <ns:description>Contains the name of the software package.</ns:description>
        </ns:column>
        <ns:column columnName="Version">
            <ns:description>Contains the version number of the software package.</ns:description>
        </ns:column>
        <ns:column columnName="Author">
            <ns:description>Contains the author of the software package.</ns:description>
        </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="SoftwareParams" tableDbType="TABLE">
      <ns:description>Contains information about the parameters passed to a particular software package used to create a particular file.
      The SoftwareId column identifies the software package used to produce the file.</ns:description>
      <ns:columns>
        <ns:column columnName="SoftwareId">
            <ns:description>Contains the unique id of the associated software package.</ns:description>
        </ns:column>
        <ns:column columnName="Name">
            <ns:description>Contains the name of the configuration parameter.</ns:description>
        </ns:column>
        <ns:column columnName="Value">
            <ns:description>Contains the value of the configuration parameter.</ns:description>
        </ns:column>
      </ns:columns>
    </ns:table>
</ns:tables>