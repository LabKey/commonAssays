<ns:tables xsi:schemaLocation="http://labkey.org/data/xml ..\..\..\..\..\schemas\tableInfo.xsd" xmlns:ns="http://labkey.org/data/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <ns:table tableName="Calibrations" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="ScanId">
            <ns:isHidden>true</ns:isHidden>
        </ns:column>
        <ns:column columnName="Name"/>
        <ns:column columnName="Value"/>
      </ns:columns>
    </ns:table>
    <ns:table tableName="Features" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="FeatureId">
            <ns:isHidden>true</ns:isHidden>
        </ns:column>
        <ns:column columnName="FileId">
            <ns:columnTitle>Features File</ns:columnTitle>
            <ns:isUnselectable>true</ns:isUnselectable>
        </ns:column>
        <ns:column columnName="Scan"/>
        <ns:column columnName="Time">
          <ns:formatString>#,##0.0000</ns:formatString>
        </ns:column>
        <ns:column columnName="MZ">
            <ns:formatString>#,##0.0000</ns:formatString>
        </ns:column>
        <ns:column columnName="AccurateMZ">
          <ns:formatString>Y;N</ns:formatString>
        </ns:column>
        <ns:column columnName="Mass">
          <ns:formatString>#,##0.0000</ns:formatString>
        </ns:column>
        <ns:column columnName="Intensity">
            <ns:formatString>#,##0.0000</ns:formatString>
        </ns:column>
        <ns:column columnName="Charge">
            <ns:columnTitle>Z</ns:columnTitle>
        </ns:column>
        <ns:column columnName="ChargeStates">
            <ns:columnTitle>Z States</ns:columnTitle>
        </ns:column>
        <ns:column columnName="KL">
            <ns:formatString>#,##0.0000</ns:formatString>
        </ns:column>
        <ns:column columnName="Background">
            <ns:formatString>#,##0.0000</ns:formatString>
        </ns:column>
        <ns:column columnName="Median">
            <ns:formatString>#,##0.0000</ns:formatString>
        </ns:column>
        <ns:column columnName="Peaks"/>
        <ns:column columnName="ScanFirst"/>
        <ns:column columnName="ScanLast"/>
        <ns:column columnName="ScanCount"/>
        <ns:column columnName="TotalIntensity">
            <ns:formatString>#,##0.0000</ns:formatString>
        </ns:column>
        <ns:column columnName="Description"/>
        <ns:column columnName="MS2Scan"/>
        <ns:column columnName="MS2ConnectivityProbability">
            <ns:formatString>0.00</ns:formatString>
            <ns:columnTitle>MS2 Prob</ns:columnTitle>
        </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="Files" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="FileId">
            <ns:isHidden>true</ns:isHidden>
        </ns:column>
        <ns:column columnName="ExpDataFileId">
            <ns:columnTitle>Experiment Data File</ns:columnTitle>
            <ns:isUnselectable>true</ns:isUnselectable>
        </ns:column>
        <ns:column columnName="Type"/>
        <ns:column columnName="MzXmlUrl"/>
        <ns:column columnName="Imported">
            <ns:isHidden>true</ns:isHidden>
        </ns:column>
          <ns:column columnName="Deleted">
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="PeakFamilies" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="PeakFamilyId">
            <ns:isHidden>true</ns:isHidden>
        </ns:column>
        <ns:column columnName="ScanId">
            <ns:isHidden>true</ns:isHidden>
            <ns:isUnselectable>true</ns:isUnselectable>
        </ns:column>
        <ns:column columnName="MzMono">
            <ns:formatString>#,##0.0000</ns:formatString>
            <ns:columnTitle>MZ Monoisotopic</ns:columnTitle>
        </ns:column>
        <ns:column columnName="Charge">
            <ns:columnTitle>Z</ns:columnTitle>
        </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="Peaks" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="PeakId">
            <ns:isHidden>true</ns:isHidden>
        </ns:column>
        <ns:column columnName="ScanId">
            <ns:columnTitle>Scan Information</ns:columnTitle>
            <ns:isUnselectable>true</ns:isUnselectable>
        </ns:column>
        <ns:column columnName="MZ">
            <ns:formatString>#,##0.0000</ns:formatString>
        </ns:column>
        <ns:column columnName="Intensity">
            <ns:formatString>#,##0.0000</ns:formatString>
        </ns:column>
        <ns:column columnName="Area">
            <ns:formatString>#,##0.00</ns:formatString>
        </ns:column>
        <ns:column columnName="Error">
            <ns:formatString>#,##0.0000</ns:formatString>
        </ns:column>
        <ns:column columnName="Frequency">
            <ns:formatString>#,##0</ns:formatString>
        </ns:column>
        <ns:column columnName="Phase">
            <ns:formatString>#,##0.00</ns:formatString>
        </ns:column>
        <ns:column columnName="Decay">
            <ns:formatString>#,##0.00</ns:formatString>
        </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="PeaksToFamilies" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="PeakFamilyId">
            <ns:columnTitle>Peak Family</ns:columnTitle>
            <ns:isUnselectable>true</ns:isUnselectable>
        </ns:column>
        <ns:column columnName="PeakId">
            <ns:isHidden>true</ns:isHidden>
            <ns:isUnselectable>true</ns:isUnselectable>
        </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="Scans" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="ScanId">
            <ns:isHidden>true</ns:isHidden>
        </ns:column>
        <ns:column columnName="FileId">
            <ns:columnTitle>Peaks File</ns:columnTitle>
            <ns:isUnselectable>true</ns:isUnselectable>
        </ns:column>
        <ns:column columnName="Scan"/>
        <ns:column columnName="RetentionTime">
            <ns:formatString>#,##0.0000</ns:formatString>
        </ns:column>
        <ns:column columnName="ObservationDuration">
            <ns:formatString>#,##0.0000</ns:formatString>
        </ns:column>
      </ns:columns>
    </ns:table>
    <ns:table tableName="Software" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="SoftwareId"/>
        <ns:column columnName="FileId"/>
        <ns:column columnName="Name"/>
        <ns:column columnName="Version"/>
        <ns:column columnName="Author"/>
      </ns:columns>
    </ns:table>
    <ns:table tableName="SoftwareParams" tableDbType="TABLE">
      <ns:columns>
        <ns:column columnName="SoftwareId"/>
        <ns:column columnName="Name"/>
        <ns:column columnName="Value"/>
      </ns:columns>
    </ns:table>
</ns:tables>