<red:redcapConfig xmlns:red="http://labkey.org/study/xml/redcapExport">
    <red:projects>
        <red:project>
            <red:serverUrl>@@serverUrl@@</red:serverUrl>
            <red:projectName>@@projectName@@</red:projectName>
            <red:subjectId>@@subjectId@@</red:subjectId>
            <red:matchSubjectIdByLabel>@@matchSubjectIdByLabel@@</red:matchSubjectIdByLabel> <!--Optional-->
            <red:demographic>@@demographic@@</red:demographic> <!--Optional-->
            <red:forms>  <!--Optional-->
                <red:form>
                    <red:formName>@@formName@@</red:formName>
                    <red:dateField>@@dateField@@</red:dateField>
                    <red:demographic>@@demographic@@</red:demographic>
                </red:form>
            </red:forms>
        </red:project>
    </red:projects>
    <red:timepointType>@@timepointType@@</red:timepointType> <!--Optional-->
    <red:duplicateNamePolicy>@@duplicateNamePolicy@@</red:duplicateNamePolicy> <!--Optional-->
</red:redcapConfig>