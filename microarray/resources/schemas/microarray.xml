<ns:tables xsi:schemaLocation="http://labkey.org/data/xml ../../../../../schemas/tableInfo.xsd" xmlns:ns="http://labkey.org/data/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <ns:table tableName="geo_properties" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="rowid"/>
      <ns:column columnName="prop_name"/>
      <ns:column columnName="category"/>
      <ns:column columnName="value"/>
      <ns:column columnName="container"/>
      <ns:column columnName="created"/>
      <ns:column columnName="createdby"/>
      <ns:column columnName="modified"/>
      <ns:column columnName="modifiedby"/>
    </ns:columns>
  </ns:table>

  <ns:table tableName="FeatureAnnotationSet" tableDbType="TABLE">
      <ns:pkColumnName>RowId</ns:pkColumnName>
      <ns:titleColumn>Name</ns:titleColumn>
      <ns:insertUrl>/feature-annotationset/upload.view</ns:insertUrl>
      <ns:importUrl></ns:importUrl>
      <ns:deleteUrl>/feature-annotationset/delete.view?rowId=${RowId}</ns:deleteUrl>
      <ns:tableUrl>/feature-annotationset/details.view?rowId=${RowId}</ns:tableUrl>
      <ns:columns>
          <ns:column columnName="RowId">
              <ns:isHidden>true</ns:isHidden>
              <ns:isKeyField>true</ns:isKeyField>
          </ns:column>
          <ns:column columnName="Container">
              <ns:isUserEditable>false</ns:isUserEditable>
              <ns:datatype>entityid</ns:datatype>
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
          <ns:column columnName="Name"/>
          <ns:column columnName="Vendor"/>
          <ns:column columnName="Description"/>
          <ns:column columnName="Created">
              <ns:description>Contains the date and time that this data was added to the system</ns:description>
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
          <ns:column columnName="CreatedBy">
              <ns:description>Contains the user that created this data</ns:description>
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
          <ns:column columnName="Modified">
              <ns:description>Contains the date and time that this data last modified</ns:description>
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
          <ns:column columnName="ModifiedBy">
              <ns:description>Contains the user that last modified this data</ns:description>
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
      </ns:columns>
  </ns:table>

  <ns:table tableName="FeatureAnnotation" tableDbType="TABLE">
      <ns:pkColumnName>RowId</ns:pkColumnName>
      <ns:titleColumn>FeatureId</ns:titleColumn>
      <ns:tableUrl></ns:tableUrl>
      <ns:deleteUrl></ns:deleteUrl>
      <ns:insertUrl></ns:insertUrl>
      <ns:importUrl></ns:importUrl>
      <ns:updateUrl></ns:updateUrl>
      <ns:columns>
          <ns:column columnName="RowId">
              <ns:isHidden>true</ns:isHidden>
              <ns:isKeyField>true</ns:isKeyField>
          </ns:column>
          <ns:column columnName="Container">
              <ns:isUserEditable>false</ns:isUserEditable>
              <ns:datatype>entityid</ns:datatype>
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
          <ns:column columnName="FeatureAnnotationSetId">
              <ns:columnTitle>Annotation Set</ns:columnTitle>
              <ns:isHidden>true</ns:isHidden>
              <ns:isUserEditable>false</ns:isUserEditable>
              <ns:fk>
                  <ns:fkTable>FeatureAnnotationSet</ns:fkTable>
              </ns:fk>
          </ns:column>
          <ns:column columnName="FeatureId">
              <ns:importAliases>
                  <ns:importAlias>ProbeId</ns:importAlias>
              </ns:importAliases>
          </ns:column>
          <ns:column columnName="GeneSymbol"/>
          <ns:column columnName="UniGeneId"/>
          <ns:column columnName="GeneId"/>
          <ns:column columnName="AccessionId"/>
          <ns:column columnName="RefSeqProteinId"/>
          <ns:column columnName="RefSeqTranscriptId"/>
          <ns:column columnName="Created">
              <ns:description>Contains the date and time that this data was added to the system</ns:description>
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
          <ns:column columnName="CreatedBy">
              <ns:description>Contains the user that created this data</ns:description>
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
          <ns:column columnName="Modified">
              <ns:description>Contains the date and time that this data last modified</ns:description>
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
          <ns:column columnName="ModifiedBy">
              <ns:description>Contains the user that last modified this data</ns:description>
              <ns:isHidden>true</ns:isHidden>
          </ns:column>
      </ns:columns>
  </ns:table>

  <ns:table tableName="FeatureData" tableDbType="TABLE">
      <ns:pkColumnName>RowId</ns:pkColumnName>
      <ns:tableUrl></ns:tableUrl>
      <ns:deleteUrl></ns:deleteUrl>
      <ns:insertUrl></ns:insertUrl>
      <ns:importUrl></ns:importUrl>
      <ns:updateUrl></ns:updateUrl>
      <ns:columns>
          <ns:column columnName="RowId">
              <ns:isHidden>true</ns:isHidden>
              <ns:isKeyField>true</ns:isKeyField>
          </ns:column>
          <ns:column columnName="Value"/>
          <ns:column columnName="FeatureId"/>
          <ns:column columnName="SampleId"/>
          <ns:column columnName="DataId"/>
      </ns:columns>
  </ns:table>

</ns:tables>
