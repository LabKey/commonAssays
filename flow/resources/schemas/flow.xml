 <ns:tables xmlns:ns="http://labkey.org/data/xml">

  <ns:table tableName="statistic" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="objectid"/>
      <ns:column columnName="statisticid"/>
      <ns:column columnName="value"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="statisticattr" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="rowid"/>
      <ns:column columnName="container"/>
      <ns:column columnName="name"/>
      <ns:column columnName="id"/>
    </ns:columns>
  </ns:table>

  <ns:table tableName="graph" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="rowid"/>
      <ns:column columnName="objectid"/>
      <ns:column columnName="graphid"/>
      <ns:column columnName="data"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="graphattr" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="rowid"/>
      <ns:column columnName="container"/>
      <ns:column columnName="name"/>
      <ns:column columnName="id"/>
    </ns:columns>
  </ns:table>

  <ns:table tableName="keyword" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="objectid"/>
      <ns:column columnName="keywordid"/>
      <ns:column columnName="value"/>
    </ns:columns>
  </ns:table>
  <ns:table tableName="keywordattr" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="rowid"/>
      <ns:column columnName="container"/>
      <ns:column columnName="name"/>
      <ns:column columnName="id"/>
    </ns:columns>
  </ns:table>

  <ns:table tableName="object" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="rowid"/>
      <ns:column columnName="container"/>
      <ns:column columnName="dataid"/>
      <ns:column columnName="typeid"/>
      <ns:column columnName="uri"/>
      <ns:column columnName="compid"/>
      <ns:column columnName="scriptid"/>
      <ns:column columnName="fcsid"/>
    </ns:columns>
  </ns:table>

  <ns:table tableName="script" tableDbType="TABLE">
    <ns:columns>
      <ns:column columnName="rowid"/>
      <ns:column columnName="objectid"/>
      <ns:column columnName="text"/>
    </ns:columns>
  </ns:table>

</ns:tables>
