<ns:tables xmlns:ns="http://labkey.org/data/xml">

    <ns:table tableName="ResultSpecimens" tableDbType="TABLE">
        <ns:columns>
            <ns:column columnName="ResultID">
            </ns:column>
            <ns:column columnName="SpecimenID">
                <ns:isReadOnly>true</ns:isReadOnly>
                <ns:isHidden>true</ns:isHidden>
            </ns:column>
            <ns:column columnName="SpecimenIndex">
                <ns:isKeyField>true</ns:isKeyField>
                <ns:isReadOnly>true</ns:isReadOnly>
                <ns:isHidden>true</ns:isHidden>
            </ns:column>
        </ns:columns>
    </ns:table>

    <ns:table tableName="Results" tableDbType="TABLE">
        <ns:pkColumnName>RowID</ns:pkColumnName>
        <ns:titleColumn>PoolID</ns:titleColumn>
        <ns:columns>
            <ns:column columnName="RowID">
                <ns:isKeyField>true</ns:isKeyField>
                <ns:isAutoInc>true</ns:isAutoInc>
                <ns:isReadOnly>true</ns:isReadOnly>
                <ns:isHidden>true</ns:isHidden>
            </ns:column>
            <ns:column columnName="Container">
                <ns:fk>
                    <ns:fkDbSchema>core</ns:fkDbSchema>
                    <ns:fkTable>Containers</ns:fkTable>
                    <ns:fkColumnName>entityid</ns:fkColumnName>
                </ns:fk>
            </ns:column>
            <ns:column columnName="ProtocolID">
                <ns:fk>
                    <ns:fkDbSchema>exp</ns:fkDbSchema>
                    <ns:fkTable>Protocol</ns:fkTable>
                    <ns:fkColumnName>RowID</ns:fkColumnName>
                </ns:fk>
            </ns:column>
            <ns:column columnName="RunID">
                <ns:isHidden>true</ns:isHidden>
                <ns:fk>
                    <ns:fkDbSchema>exp</ns:fkDbSchema>
                    <ns:fkTable>experimentrun</ns:fkTable>
                    <ns:fkColumnName>RowID</ns:fkColumnName>
                </ns:fk>
            </ns:column>
            <ns:column columnName="DataID">
                <ns:fk>
                    <ns:fkDbSchema>exp</ns:fkDbSchema>
                    <ns:fkTable>Data</ns:fkTable>
                    <ns:fkColumnName>RowID</ns:fkColumnName>
                </ns:fk>
            </ns:column>
            <ns:column columnName="ObjectID">
                <ns:fk>
                    <ns:fkDbSchema>exp</ns:fkDbSchema>
                    <ns:fkTable>Object</ns:fkTable>
                    <ns:fkColumnName>ObjectID</ns:fkColumnName>
                </ns:fk>
            </ns:column>

            <ns:column columnName="SampleNum"/>
            <ns:column columnName="ParticipantID"/>
            <ns:column columnName="VisitID"/>
            <ns:column columnName="Date"/>

            <ns:column columnName="PoolID"/>
            <ns:column columnName="TotalCells"/>
            <ns:column columnName="ViableCells"/>

            <!-- calculated specimen aggregates -->
            <ns:column columnName="SpecimenAggregatesUpdated"/>
            <ns:column columnName="SpecimenCount"/>
            <ns:column columnName="SpecimenMatchCount"/>
            <ns:column columnName="SpecimenIDs"/>
            <ns:column columnName="SpecimenMatches"/>
            <ns:column columnName="OriginalCells"/>
        </ns:columns>
    </ns:table>

</ns:tables>
