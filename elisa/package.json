{
  "name": "elisa",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "setup": "npm ci",
    "build": "npm run build-dev",
    "start-link": "cross-env LINK=true npm run start",
    "start": "cross-env NODE_ENV=development LK_MODULE_CONTAINER=commonAssays LK_MODULE=elisa webpack-dev-server --config node_modules/@labkey/build/webpack/watch.config.js",
    "build-dev": "npm run clean && cross-env NODE_ENV=development LK_MODULE_CONTAINER=commonAssays LK_MODULE=elisa webpack --config node_modules/@labkey/build/webpack/dev.config.js --progress --profile --colors",
    "build-prod": "npm run clean && cross-env NODE_ENV=production PROD_SOURCE_MAP=source-map LK_MODULE_CONTAINER=commonAssays LK_MODULE=elisa webpack --config node_modules/@labkey/build/webpack/prod.config.js --progress --profile --colors",
    "clean": "rimraf resources/web/elisa/gen",
    "test": "cross-env NODE_ENV=test jest",
    "teamcity": "cross-env NODE_ENV=test jest --testResultsProcessor=jest-teamcity-reporter"
  },
  "jest": {
    "globals": {
      "LABKEY": {}
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "setupFilesAfterEnv": [
      "./test/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "preset": "ts-jest",
    "testMatch": null,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/test/styleMock.ts"
    },
    "testResultsProcessor": "jest-teamcity-reporter"
  },
  "dependencies": {
    "@labkey/build": "0.0.1-fb-labkeyBuildPackage.5",
    "@labkey/components": "0.97.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.1",
    "@babel/polyfill": "7.10.1",
    "@babel/preset-env": "7.10.1",
    "@babel/preset-react": "7.10.1",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@labkey/eslint-config-base": "0.0.8",
    "@labkey/eslint-config-react": "0.0.8",
    "@types/jest": "25.2.3",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "bootstrap-sass": "3.4.1",
    "cross-env": "7.0.2",
    "css-loader": "3.5.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.5.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "jest": "26.0.1",
    "jest-cli": "24.9.0",
    "jest-teamcity-reporter": "0.9.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "4.12.21",
    "react-test-renderer": "16.8.6",
    "resolve-url-loader": "3.1.1",
    "rimraf": "3.0.2",
    "sass-loader": "8.0.2",
    "style-loader": "1.2.1",
    "ts-jest": "26.0.0",
    "ts-loader": "7.0.5",
    "typescript": "3.9.3",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0"
  }
}
