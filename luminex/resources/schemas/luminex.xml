<ns:tables xsi:schemaLocation="http://labkey.org/data/xml ..\..\..\..\schemas\tableInfo.xsd" xmlns:ns="http://labkey.org/data/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <ns:table tableName="Analyte" tableDbType="TABLE">
        <ns:columns>
            <ns:column columnName="RowId"/>
            <ns:column columnName="LSID"/>
            <ns:column columnName="Name"/>
            <ns:column columnName="DataId"/>
            <ns:column columnName="FitProb"/>
            <ns:column columnName="ResVar"/>
            <ns:column columnName="RegressionType"/>
            <ns:column columnName="StdCurve"/>
            <ns:column columnName="MinStandardRecovery"/>
            <ns:column columnName="MaxStandardRecovery"/>
        </ns:columns>
    </ns:table>

    <ns:table tableName="DataRow" tableDbType="TABLE">
        <ns:columns>
            <ns:column columnName="RowId"/>
            <ns:column columnName="DataId"/>
            <ns:column columnName="AnalyteId"/>
            <ns:column columnName="Type"/>
            <ns:column columnName="Well"/>
            <ns:column columnName="Outlier"/>
            <ns:column columnName="Description"/>
            <ns:column columnName="PTID"/>
            <ns:column columnName="VisitID"/>
            <ns:column columnName="Date"/>
            <ns:column columnName="FIString"/>
            <ns:column columnName="FI">
                <ns:formatString>0.0</ns:formatString>
            </ns:column>
            <ns:column columnName="FIOORIndicator"/>
            <ns:column columnName="FIBackgroundString"/>
            <ns:column columnName="FIBackground">
                <ns:formatString>0.0</ns:formatString>
                <ns:columnTitle>FI - Bkgd</ns:columnTitle>
            </ns:column>
            <ns:column columnName="FIBackgroundOORIndicator"/>
            <ns:column columnName="StdDevString"/>
            <ns:column columnName="StdDev">
                <ns:formatString>0.00</ns:formatString>
            </ns:column>
            <ns:column columnName="StdDevOORIndicator"/>
            <ns:column columnName="ObsConcString"/>
            <ns:column columnName="ObsConc">
                <ns:formatString>0.00</ns:formatString>
            </ns:column>
            <ns:column columnName="ObsConcOORIndicator"/>
            <ns:column columnName="ExpConc">
                <ns:formatString>0.00</ns:formatString>
            </ns:column>
            <ns:column columnName="ObsOverExp">
                <ns:formatString>0</ns:formatString>
                <ns:columnTitle>(Obs/Exp) * 100</ns:columnTitle>
            </ns:column>
            <ns:column columnName="ConcInRangeString"/>
            <ns:column columnName="ConcInRange">
                <ns:formatString>0.00</ns:formatString>
            </ns:column>
            <ns:column columnName="ConcInRangeOORIndicator"/>
            <ns:column columnName="Dilution"/>
            <ns:column columnName="DataRowGroup"/>
            <ns:column columnName="Ratio"/>
            <ns:column columnName="SamplingErrors"/>
            <ns:column columnName="ExtraSpecimenInfo"/>
            <ns:column columnName="SpecimenID"/>
            <ns:column columnName="Container"/>
            <ns:column columnName="ProtocolID"/>
        </ns:columns>
    </ns:table>


</ns:tables>